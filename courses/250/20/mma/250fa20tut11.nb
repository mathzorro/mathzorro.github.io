34379bd0861e"],

Cell[TextData[{
 StyleBox["RegionPlot3D", "Input",
  FontSize->16],
 " is especially useful for finding the intersection of two objects."
}], "Text",
 CellChangeTimes->{{3.635007713656707*^9, 3.635007720977385*^9}, {
  3.8104878754516177`*^9, 
  3.8104879068215313`*^9}},ExpressionUUID->"d31209d9-0028-470f-bc4e-\
536ca192795b"],

Cell[TextData[{
 StyleBox["Example 11.5.1. ",
  FontWeight->"Bold"],
 "What is the region inside two spheres?"
}], "Text",
 CellChangeTimes->{{3.635007713656707*^9, 3.635007720977385*^9}, {
  3.8104878754516177`*^9, 3.8104879117542753`*^9}, {3.8105497937776184`*^9, 
  3.8105497938615923`*^9}},ExpressionUUID->"91bd73ef-fcb6-4e55-890b-\
98547d8e9ee4"],

Cell[BoxData[
 RowBox[{"spheres", "=", 
  RowBox[{"Graphics3D", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Opacity", "[", ".3", "]"}], ",", "Red", ",", 
     RowBox[{"Sphere", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", "1"}], "]"}], ",", 
     "Blue", ",", 
     RowBox[{"Sphere", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", "0", ",", "0"}], "}"}], ",", "1"}], "]"}]}], "}"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.635007895622221*^9, 3.6350078997984333`*^9}},
 CellLabel->
  "In[261]:=",ExpressionUUID->"1bdfc560-9e8d-4041-baf2-045898c45c7c"],

Cell[BoxData[
 RowBox[{"intersection", "=", 
  RowBox[{"RegionPlot3D", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"x", "^", "2"}], "+", 
        RowBox[{"y", "^", "2"}], "+", 
        RowBox[{"z", "^", "2"}]}], "\[LessEqual]", "1"}], ")"}], "&&", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x", "-", "1"}], ")"}], "^", "2"}], "+", 
        RowBox[{"y", "^", "2"}], "+", 
        RowBox[{"z", "^", "2"}]}], "\[LessEqual]", "1"}], ")"}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"-", "1"}], ",", "2"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"y", ",", 
      RowBox[{"-", "1.5"}], ",", "1.5"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"z", ",", 
      RowBox[{"-", "1.5"}], ",", "1.5"}], "}"}], ",", 
    RowBox[{"PlotPoints", "\[Rule]", "50"}], ",", 
    RowBox[{"MeshStyle", "\[Rule]", "None"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6350076773017397`*^9, 3.635007678491878*^9}, {
  3.635007723737496*^9, 3.63500777388496*^9}, {3.635007835885652*^9, 
  3.635007846445241*^9}, {3.635007890807876*^9, 3.6350078922862873`*^9}, {
  3.729337911511795*^9, 3.729337924206958*^9}, {3.810486966766227*^9, 
  3.810486978603589*^9}},
 CellLabel->
  "In[264]:=",ExpressionUUID->"d727100e-d1c0-4f3c-badc-778bc8f96148"],

Cell[BoxData[
 RowBox[{"Show", "[", 
  RowBox[{"spheres", ",", "intersection"}], "]"}]], "Input",
 CellChangeTimes->{{3.635007787596767*^9, 3.635007821469208*^9}, {
  3.6350078546195717`*^9, 3.6350078746633883`*^9}, {3.63500790685791*^9, 
