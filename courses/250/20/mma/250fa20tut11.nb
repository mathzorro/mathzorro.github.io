      RowBox[{"2", "t"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", 
       RowBox[{"2", "Pi"}]}], "}"}], ",", 
     RowBox[{"PlotLabel", "\[Rule]", "\"\<y-coordinate\>\""}]}], "]"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"Sin", "[", 
      RowBox[{"3", "t"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", 
       RowBox[{"2", "Pi"}]}], "}"}], ",", 
     RowBox[{"PlotLabel", "\[Rule]", "\"\<z-coordinate\>\""}]}], "]"}]}], 
  "}"}]], "Input",
 CellChangeTimes->{{3.7293348290985622`*^9, 3.7293349090595922`*^9}},
 CellLabel->
  "In[256]:=",ExpressionUUID->"9a03dc6e-086a-4219-b859-b84d2480ccbb"],

