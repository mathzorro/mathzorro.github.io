Cell["\<\
In the tutorial about Manipulate, you have seen a number of different types \
of Control Objects. You can use each of them individually outside of \
Manipulate.\
\>", "Text",
 CellChangeTimes->{{3.8908368206504793`*^9, 
  3.8908368749142303`*^9}},ExpressionUUID->"40adb86d-be2c-4bcf-a993-\
a9490a872d41"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"DynamicModule", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"v1", "=", "False"}], ",", 
     RowBox[{"v2", "=", "0"}], ",", "v3", ",", "v4"}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"Column", "[", 
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Row", "[", 
       RowBox[{"{", 
        RowBox[{"\"\<Check me! \>\"", ",", " ", 
         RowBox[{"Checkbox", "[", 
          RowBox[{"Dynamic", "[", "v1", "]"}], "]"}], ",", 
         RowBox[{"Dynamic", "[", 
          RowBox[{"If", "[", 
           RowBox[{"v1", ",", 
            RowBox[{"Style", "[", 
             RowBox[{"\"\<  You did it!\>\"", ",", 
              RowBox[{"FontFamily", "->", "\"\<Arial\>\""}], ",", "Bold", ",",
               " ", "Red"}], "]"}], ",", "\"\<\>\""}], "]"}], "]"}]}], "}"}], 
       "]"}], ",", "\[IndentingNewLine]", 
      RowBox[{"Row", "[", 
       RowBox[{"{", 
        RowBox[{"\"\<Set me! \>\"", ",", " ", 
         RowBox[{"SetterBar", "[", 
          RowBox[{
           RowBox[{"Dynamic", "[", "v2", "]"}], ",", 
           RowBox[{"Range", "[", "5", "]"}]}], "]"}], ",", 
         RowBox[{"Dynamic", "[", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"v2", "<", "5"}], ",", 
            RowBox[{"Style", "[", 
             RowBox[{"\"\<  Too low!\>\"", ",", 
              RowBox[{"FontFamily", "->", "\"\<Arial\>\""}], ",", " ", 
              "Blue"}], "]"}], ",", 
            RowBox[{"Style", "[", 
             RowBox[{"\"\<  You got it!\>\"", ",", 
              RowBox[{"FontFamily", "->", "\"\<Arial\>\""}], ",", "Bold", ",",
               " ", "Red"}], "]"}]}], "]"}], "]"}]}], "}"}], "]"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"Row", "[", 
       RowBox[{"{", 
        RowBox[{"\"\<Slide me! \>\"", ",", " ", 
         RowBox[{"Slider", "[", 
          RowBox[{
           RowBox[{"Dynamic", "[", "v3", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "1"}], "}"}], ",", 
           RowBox[{"ImageSize", "->", "100"}]}], "]"}], ",", 
         RowBox[{"Dynamic", "[", 
          RowBox[{"Which", "[", 
           RowBox[{
            RowBox[{"v3", "==", "0"}], ",", "\"\<\>\"", ",", 
            RowBox[{"v3", "<", "1"}], ",", 
            RowBox[{"Style", "[", 
             RowBox[{"\"\<  Keep Going!\>\"", ",", 
              RowBox[{"FontFamily", "->", "\"\<Arial\>\""}], ",", " ", 
              "Blue"}], "]"}], ",", 
            RowBox[{"v3", "==", "1"}], ",", 
            RowBox[{"Style", "[", 
             RowBox[{"\"\<  You got it!\>\"", ",", 
              RowBox[{"FontFamily", "->", "\"\<Arial\>\""}], ",", "Bold", ",",
               " ", "Red"}], "]"}]}], "]"}], "]"}]}], "}"}], "]"}]}], 
     "\[IndentingNewLine]", "}"}], "]"}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
 CellChangeTimes->{{3.890836908320689*^9, 3.890837084785616*^9}, {
  3.8908371164548903`*^9, 3.8908373388524218`*^9}, {3.890837371193181*^9, 
  3.8908374131251297`*^9}, {3.890837443888208*^9, 3.890837444426133*^9}},
 CellLabel->"In[85]:=",ExpressionUUID->"dc22b89b-3749-42bf-91f5-3af042f8b909"],

Cell[BoxData[
 DynamicModuleBox[{$CellContext`v1$$ = False, $CellContext`v2$$ = 
  0, $CellContext`v3$$ = 0., $CellContext`v4$$}, 
  TagBox[GridBox[{
     {
      TemplateBox[{"\"Check me! \"", 
        CheckboxBox[
         Dynamic[$CellContext`v1$$]], 
        DynamicBox[
         ToBoxes[
          If[$CellContext`v1$$, 
           Style["  You did it!", FontFamily -> "Arial", Bold, Red], ""], 
          StandardForm]]},
       "RowDefault"]},
     {
      TemplateBox[{"\"Set me! \"", 
        InterpretationBox[
         StyleBox[
          GridBox[{{
             SetterBox[
              Dynamic[$CellContext`v2$$], {1}, "1"], 
             SetterBox[
              Dynamic[$CellContext`v2$$], {2}, "2"], 
             SetterBox[
              Dynamic[$CellContext`v2$$], {3}, "3"], 
             SetterBox[
              Dynamic[$CellContext`v2$$], {4}, "4"], 
             SetterBox[
              Dynamic[$CellContext`v2$$], {5}, "5"]}}, ColumnSpacings -> 0, 
           BaselinePosition -> {1, 1}], Deployed -> True], 
         SetterBar[
          Dynamic[$CellContext`v2$$], {1, 2, 3, 4, 5}]], 
        DynamicBox[
         ToBoxes[
          If[$CellContext`v2$$ < 5, 
           Style["  Too low!", FontFamily -> "Arial", Blue], 
           Style["  You got it!", FontFamily -> "Arial", Bold, Red]], 
          StandardForm]]},
       "RowDefault"]},
     {
      TemplateBox[{"\"Slide me! \"", 
        SliderBox[
         Dynamic[$CellContext`v3$$], {0, 1}, ImageSize -> 100], 
        DynamicBox[
         ToBoxes[
          Which[$CellContext`v3$$ == 0, "", $CellContext`v3$$ < 1, 
           Style[
           "  Keep Going!", FontFamily -> "Arial", Blue], $CellContext`v3$$ == 
           1, 
           Style["  You got it!", FontFamily -> "Arial", Bold, Red]], 
          StandardForm]]},
       "RowDefault"]}
    },
    DefaultBaseStyle->"Column",
    GridBoxAlignment->{"Columns" -> {{Left}}},
    GridBoxItemSize->{"Columns" -> {{Automatic}}, "Rows" -> {{Automatic}}}],
   "Column"],
  DynamicModuleValues:>{}]], "Output",
 CellChangeTimes->{{3.890837011670589*^9, 3.890837078207364*^9}, 
   3.8908371260590076`*^9, {3.890837266071828*^9, 3.8908373393014307`*^9}, 
   3.890837374759686*^9, 3.89083741369748*^9, 3.8908374588078403`*^9},
 CellLabel->"Out[85]=",ExpressionUUID->"5b47d6e6-5cda-4715-a2d7-53c0adbe81c1"]
}, Open  ]],

