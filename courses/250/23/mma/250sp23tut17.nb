Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"y", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Column", "[", 
  RowBox[{"{", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Button", "[", 
     RowBox[{"\"\<Add one to y\>\"", ",", 
      RowBox[{"y", "++"}]}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Button", "[", 
     RowBox[{"\"\<Subtract one from y\>\"", ",", 
      RowBox[{"y", "--"}]}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Button", "[", 
     RowBox[{"\"\<Square y\>\"", ",", 
      RowBox[{"y", "=", 
       RowBox[{"y", "*", "y"}]}]}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Button", "[", 
     RowBox[{"\"\<Reset y to 3\>\"", ",", 
      RowBox[{"y", "=", "3"}]}], "]"}]}], "\[IndentingNewLine]", "}"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Dynamic", "[", "\[IndentingNewLine]", 
  RowBox[{"Framed", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Style", "[", 
     RowBox[{
      RowBox[{"\"\< y is \>\"", "<>", 
       RowBox[{"ToString", "[", "y", "]"}], "<>", "\"\< \>\""}], ",", 
      RowBox[{"FontSize", "\[Rule]", "30"}], ",", 
      RowBox[{"FontFamily", "\[Rule]", "\"\<Arial\>\""}], ",", " ", "Bold", 
      ",", " ", 
      RowBox[{"Darker", "[", "Green", "]"}]}], "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Background", "\[Rule]", "LightYellow"}]}], "]"}], 
  "]"}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.750589220749487*^9, 3.750589463528592*^9}, {
