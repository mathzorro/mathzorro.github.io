997c97c9ddb4"],

Cell[BoxData[{
 RowBox[{"radius", "=", "1"}], "\[IndentingNewLine]", 
 RowBox[{"ngon", "=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", "=", "5"}], "}"}], ",", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"radius", "*", 
         RowBox[{"Cos", "[", 
          RowBox[{"2", "Pi", " ", 
           RowBox[{"i", "/", "n"}]}], "]"}]}], ",", 
        RowBox[{"radius", "*", 
         RowBox[{"Sin", "[", 
          RowBox[{"2", " ", "Pi", " ", 
           RowBox[{"i", "/", "n"}]}], "]"}]}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
