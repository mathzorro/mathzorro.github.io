Cell["\<\
If you want to create a button for your users, use the Button command.  Here \
is an example of clicking the button to change a variable.  Notice the \
multiple times that the Dynamic command is used. \
\>", "Text",
 CellChangeTimes->{{3.75058919230597*^9, 3.750589216571033*^9}, {
  3.750589267352068*^9, 3.750589278910297*^9}, {3.750589507530116*^9, 
  3.750589527791437*^9}, {3.7824616917432485`*^9, 
  3.782461738248873*^9}},ExpressionUUID->"528dfec6-104b-424d-87e4-\
8ddf9e228e23"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"x", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Column", "[", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"Button", "[", 
     RowBox[{"\"\<Add one to x\>\"", ",", 
      RowBox[{"Dynamic", "[", 
       RowBox[{"x", "++"}], "]"}]}], "]"}], ",", 
    RowBox[{"Button", "[", 
     RowBox[{"\"\<Add subtract one from x\>\"", ",", 
      RowBox[{"Dynamic", "[", 
       RowBox[{"x", "--"}], "]"}]}], "]"}], ",", 
    RowBox[{"Button", "[", 
     RowBox[{"\"\<Square x\>\"", ",", 
      RowBox[{"Dynamic", "[", 
       RowBox[{"x", "=", 
        RowBox[{"x", "*", "x"}]}], "]"}]}], "]"}], ",", 
    RowBox[{"Button", "[", 
     RowBox[{"\"\<Reset x to 3\>\"", ",", 
      RowBox[{"Dynamic", "[", 
       RowBox[{"x", "=", "3"}], "]"}]}], "]"}]}], "}"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Dynamic", "[", 
  RowBox[{"Framed", "[", 
   RowBox[{
    RowBox[{"Style", "[", 
     RowBox[{
      RowBox[{"\"\<x is \>\"", "<>", 
       RowBox[{"ToString", "[", "x", "]"}]}], ",", 
      RowBox[{"FontSize", "\[Rule]", "30"}], ",", 
      RowBox[{"FontFamily", "\[Rule]", "\"\<Arial\>\""}], ",", " ", "Bold", 
      ",", " ", 
      RowBox[{"Darker", "[", "Green", "]"}]}], "]"}], ",", 
    RowBox[{"Background", "\[Rule]", "LightYellow"}]}], "]"}], 
  "]"}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.750589220749487*^9, 3.750589463528592*^9}, {
  3.750589543168693*^9, 3.750589624648654*^9}},
 CellLabel->"In[84]:=",ExpressionUUID->"b01c592f-5ce4-40b7-b33c-9a5a61839dc2"],

Cell[BoxData[
 TagBox[GridBox[{
    {
     ButtonBox["\<\"Add one to x\"\>",
      Appearance->Automatic,
      ButtonFunction:>Dynamic[
        Increment[$CellContext`x]],
      Evaluator->Automatic,
      Method->"Preemptive"]},
    {
     ButtonBox["\<\"Add subtract one from x\"\>",
      Appearance->Automatic,
      ButtonFunction:>Dynamic[
        Decrement[$CellContext`x]],
      Evaluator->Automatic,
      Method->"Preemptive"]},
    {
     ButtonBox["\<\"Square x\"\>",
      Appearance->Automatic,
      ButtonFunction:>Dynamic[$CellContext`x = $CellContext`x $CellContext`x],
      
      Evaluator->Automatic,
      Method->"Preemptive"]},
    {
     ButtonBox["\<\"Reset x to 3\"\>",
      Appearance->Automatic,
      ButtonFunction:>Dynamic[$CellContext`x = 3],
      Evaluator->Automatic,
      Method->"Preemptive"]}
   },
   DefaultBaseStyle->"Column",
   GridBoxAlignment->{"Columns" -> {{Left}}},
   GridBoxItemSize->{"Columns" -> {{Automatic}}, "Rows" -> {{Automatic}}}],
  "Column"]], "Output",
