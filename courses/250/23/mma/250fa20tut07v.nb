Cell[TextData[{
 "That is, when you type ",
 StyleBox["If[", "Input",
  FontSize->14],
 StyleBox["condition",
  FontSlant->"Italic"],
 ", ",
 StyleBox["expression1",
  FontSlant->"Italic"],
 ", ",
 StyleBox["expression2",
  FontSlant->"Italic"],
 StyleBox["]", "Input",
  FontSize->14],
 ", ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " will first check the ",
 StyleBox["condition",
  FontSlant->"Italic"],
 ".  If the ",
 StyleBox["condition",
  FontSlant->"Italic"],
 " evaluates to ",
 StyleBox["True", "Input",
  FontSize->16],
 ", then it will do ",
 StyleBox["expression1",
  FontSlant->"Italic"],
 "; whereas, if it evaluates to ",
 StyleBox["False", "Input",
  FontSize->16],
 ", it will do ",
 StyleBox["expression2.",
  FontSlant->"Italic"]
}], "Text",
 CellChangeTimes->{{3.462185595882409*^9, 3.4621857149009867`*^9}, {
