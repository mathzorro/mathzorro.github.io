ba6288415c2d"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"squares", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"i", "^", "2"}], ",", 
     RowBox[{"{", "5", "}"}]}], "]"}]}], ";"}], "\n", 
