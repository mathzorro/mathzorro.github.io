Cell[TextData[{
 "When there is only one input ",
 StyleBox["n",
  FontSlant->"Italic"],
 ", the output will be a list of integers starting at 1 and increasing to ",
 StyleBox["n",
  FontSlant->"Italic"],
 ".\nWhen there are two inputs ",
 StyleBox["m",
  FontSlant->"Italic"],
 " and ",
 StyleBox["n",
  FontSlant->"Italic"],
 ", the output will be the list of integers starting at ",
 StyleBox["m",
  FontSlant->"Italic"],
 " and increasing to ",
 StyleBox["n",
  FontSlant->"Italic"],
 ".\nWhen there are three inputs ",
 StyleBox["m",
  FontSlant->"Italic"],
 ", ",
 StyleBox["n",
  FontSlant->"Italic"],
 ", and ",
 StyleBox["incr",
  FontSlant->"Italic"],
 ", then the output will be the list of integers starting at ",
 StyleBox["m",
  FontSlant->"Italic"],
 " and increasing to ",
 StyleBox["n",
  FontSlant->"Italic"],
 " in increments of ",
 StyleBox["incr",
  FontSlant->"Italic"],
 "."
}], "Text",
